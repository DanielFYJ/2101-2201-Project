@startuml
skinparam sequenceArrowThickness 2
skinparam roundcorner 20
skinparam maxmessagesize 60

actor Kids
participant "//<<Boundary>>//\n**WebPortalUI**" as A
participant "//<<Control>>//\n**QueueManagement**" as B
participant "//<<Entity>>//\n**Queue**" as C

Kids-> A: ViewQueue
activate A #FFBBBB

A -> B: getQueue()
activate B #FFBBBB

B -> C: getMoveCommand()
activate C #FFBBBB
C --> B: Success()
deactivate C

B --> A: Success()
deactivate B 

A -> Kids: "Queue Displayed Successfully"
deactivate A

@enduml