@startuml
skinparam sequenceArrowThickness 2
skinparam roundcorner 20
skinparam maxmessagesize 60

actor Kid
participant "//<<Boundary>>//\n**WebPortalUI**" as A
participant "//<<Control>>//\n**QueueManagement**" as B
participant "//<<Entity>>//\n**Queue**" as C

Kid-> A: ViewQueue
activate A

A -> B: getQueue()
activate B

B -> C: getMoveCommand()
activate C
C --> B: Success()
deactivate C

B --> A: Success()
deactivate B

A --> Kid: "Queue Displayed Successfully"
deactivate A

@enduml