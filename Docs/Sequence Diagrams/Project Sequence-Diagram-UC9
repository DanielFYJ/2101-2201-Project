@startuml
actor RoboticCar
actor Kid
participant "//<<control>>//\n**RoboticCarManagement**" as B
participant "//<<control>>//\n**CarCommunicationManagement**" as D
participant "//<<control>>//\n**WebCommunicationManagement**" as E
participant "//<<control>>//\n**FeedbackManagement**" as H
participant "//<<entity>>//\n**Dashboard**" as I
database Database 

Kid-> B: View feedback
activate B #FFBBBB

B-> E: receiveFeedback()
activate E #FFBBBB

E-> H: getFeedback(feedback)
activate H #FFBBBB

H-> I: getFeedback()
activate I #FFBBBB

I-> Database: Retrieve feedback
activate Database #FFBBBB

Database-->I: Success()
deactivate Database

I-->H: Success()
deactivate I

H-->E: Success()
deactivate H

E-->B: Success()
deactivate E

B-->Kid: Success()
deactivate B

RoboticCar -> B: Send robotic car movement
activate B #FFBBBB

alt Successful connection between web portal and robotic car
B -> D: isConnectedtoWeb()
activate D #FFBBBB 

D --> B: Success()
deactivate D

B -> D: sendFeedback(sensorData, errorMessage)
activate D #FFBBBB 

D -> E: receiveFeedback()
activate E #FFBBBB

E -> H: getFeedback(feedback)
activate H #FFBBBB

H -> I: getFeedback()
activate I #FFBBBB

I -> Database: Retrieve feedback
activate Database #FFBBBB

Database --> I: Success()
deactivate Database

I --> H: Success()
deactivate I

H --> E: Success()
deactivate H

E --> D: Success()
deactivate E

D --> B: Success()
deactivate D

B --> RoboticCar: Success()

else Failed connection between web portal and robotic car
B -> D: isConnectedtoWeb()
activate D #FFBBBB 

D --> B: Failure()
deactivate D

B -> D: sendFeedback(sensorData, errorMessage)
activate D #FFBBBB

D -> E: receiveFeedback()
activate E #FFBBBB

E -> H: getFeedback(feedback)
activate H #FFBBBB

H -> I: getFeedback()
activate I #FFBBBB

I -> Database: Retrieve feedback
activate Database #FFBBBB

Database --> I: Success()
deactivate Database

I --> H: Success()
deactivate I

H --> E: Success()
deactivate H

E --> D: Success()
deactivate E

D --> B: Success()
deactivate D

B --> RoboticCar: Success()
deactivate B
end
@enduml